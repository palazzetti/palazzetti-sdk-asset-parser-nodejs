{
  "name": "palazzetti-sdk-asset-parser",
  "version": "17.0.2",
  "description": "",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "exports": {
    "require": "./lib/cjs/index.js",
    "import": "./lib/esm/index.js"
  },
  "types": "./lib/types/index.d.ts",
  "files": [
    "lib/"
  ],
  "scripts": {
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "clean": "rimraf lib",
    "declarations": "tsc -p tsconfig.json",
    "build:esm": "cross-env BABEL_ENV=esmUnbundled babel src --extensions '.ts' --out-dir 'lib/esm' --source-maps",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --extensions '.ts' --out-dir 'lib/cjs' --source-maps",
    "build:bundles": "cross-env BABEL_ENV=esmBundled rollup -c",
    "build": "npm-run-all -l clean -p build:esm build:cjs build:bundles declarations"
  },
  "repository": {
    "type": "git",
    "url": "git+https://palazzetti@github.com/palazzetti/palazzetti-sdk-asset-parser-nodejs.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/palazzetti/palazzetti-sdk-asset-parser-nodejs/issues"
  },
  "homepage": "https://github.com/palazzetti/palazzetti-sdk-asset-parser-nodejs#readme",
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.12",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@types/node": "^14.14.20",
    "@types/semver-compare": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "cjstoesm": "^1.1.4",
    "cross-env": "^7.0.3",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.66.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-ts": "^2.0.5",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@rollup/plugin-json": "^4.1.0",
    "semver-compare": "^1.0.0"
  }
}
